{
  "name": "quasi-peer-system",
  "version": "1.0.0",
  "description": "Distributed quasi-peer system for AI-driven WebRTC features",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test:integration": "jest --config jest.integration.config.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "k8s:apply": "kubectl apply -f k8s/",
    "keygen": "ts-node src/tools/keygen.ts"
  },
  "dependencies": {
    "@xenova/transformers": "^2.15.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "mediasoup": "^3.12.0",
    "prom-client": "^14.2.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "winston": "^3.10.0",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-typescript": "^7.26.0",
    "@types/express": "^4.17.17",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.75",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.38.0",
    "jest": "^29.5.0",
    "mediasoup-client": "^3.7.0",
    "prettier": "^2.8.7",
    "socket.io-client": "^4.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
